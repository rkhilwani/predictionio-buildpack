<% if ENV["PIO_ELASTICSEARCH_URL"] %>
spark.es.net.http.auth.user=<%= ENV["PIO_ELASTICSEARCH_USERNAME"] %>
spark.es.net.http.auth.pass=<%= ENV["PIO_ELASTICSEARCH_PASSWORD"] %>
spark.es.nodes=<%= ENV["PIO_ELASTICSEARCH_HOSTS"] %>
spark.es.nodes.wan.only=true
spark.es.port=<%= ENV["PIO_ELASTICSEARCH_PORTS"] %>
spark.es.net.ssl=<%= ENV["PIO_ELASTICSEARCH_SCHEMES"] == "https" %>
<% end %>